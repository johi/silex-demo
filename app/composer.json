    "symfony/config": "v2.7.4",
    "symfony/yaml": "v2.7.4",
    "silex/silex": "v1.3.3",
    "twig/twig": "v1.21.1",
    "monolog/monolog": "1.17.1",
    "symfony/twig-bridge": "v2.7.4",
    "symfony/form": "v2.7.4",
    "symfony/validator": "v2.7.4",
    "symfony/security-csrf": "v2.7.4",
    "swiftmailer/swiftmailer": "v5.4.1",
    "psr/log": "1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.6"
  },
  "minimum-stability": "stable",
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-4": {
      "johi\\SilexDemo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\johi\\SilexDemo\\": "test"
    }
